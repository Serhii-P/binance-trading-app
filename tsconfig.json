{
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./typings"],
    "jsx": "react-jsx",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "target": "es5",
    "types": [
      "node",
      "react",
      "react-dom",
      "@types/apexcharts"
    ],
  },
  "include": ["src"]
}